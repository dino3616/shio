  "license": "MIT",
  "private": true,
  "type": "module",
  "module": "nodenext",
  "scripts": {
    "generate": "graphql-codegen --config codegen.ts -r dotenv/config",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "style": "stylelint ./**/*.{css,scss}",
    "style:fix": "stylelint ./**/*.{css,scss} --fix",
    "test": "jest --silent=false --verbose false --passWithNoTests",
    "gql:introspect": "graphql-codegen --config codegen-introspect.ts -r dotenv/config"
  },
  "dependencies": {
    "@shio/core": "workspace:*",
    "@shio/design-token": "workspace:*",
    "@shio/tailwind": "workspace:*",
    "@vercel/analytics": "1.3.1",
    "dotenv": "16.4.5",
    "framer-motion": "10.18.0",
    "graphql-tag": "2.12.6",
    "next": "13.5.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.1.0",
    "@graphql-codegen/typescript-operations": "4.3.0",
    "@graphql-codegen/typescript-urql": "4.0.0",
    "@graphql-codegen/urql-introspection": "3.0.0",
    "@shio/jest": "workspace:*",
    "@shio/postcss": "workspace:*",
    "@shio/prettier": "workspace:*",
    "@shio/stylelint": "workspace:*",
    "@shio/tsconfig": "workspace:*",
    "@shio/type": "workspace:*",
    "@storybook/react": "7.6.20",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "eslint-config-shio-nextjs": "workspace:*"
  }
