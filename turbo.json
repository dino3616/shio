{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "generate": {},
    "lint": {
      "dependsOn": ["^generate", "generate"]
    },
    "fmt": {
      "dependsOn": ["^generate", "generate"]
    },
    "style": {
      "dependsOn": ["^generate", "generate"]
    },
    "build": {
      "dependsOn": ["^generate", "generate", "^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "test": {
      "dependsOn": ["^build", "build"]
    }
  }
}
